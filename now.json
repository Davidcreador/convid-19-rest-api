{
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/confirmed",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/recovered",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/deaths",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/latest",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api-docs",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    }
  ]
}
